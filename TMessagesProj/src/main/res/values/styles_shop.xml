<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="ShopBotNameList">
        <item name="android:fontFamily">sans-serif-medium</item>
        <item name="android:ellipsize">end</item>
        <item name="android:maxLines">1</item>
        <item name="android:textColor">#000</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textStyle">normal</item>
    </style>

    <style name="ShopBotNameGrid">
        <item name="android:fontFamily">sans-serif-medium</item>
        <item name="android:ellipsize">end</item>
        <item name="android:maxLines">1</item>
        <item name="android:textColor">#000</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textStyle">normal</item>
    </style>

    <style name="ShopBotStatusText">
        <item name="android:fontFamily">sans-serif</item>
        <item name="android:ellipsize">end</item>
        <item name="android:maxLines">1</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">@color/colorShopStatusText</item>
    </style>

    <style name="ShopNewLabel">
        <item name="android:background">@drawable/bg_new</item>
        <item name="android:fontFamily">sans-serif-medium</item>
        <item name="android:textColor">@color/colorShopButtonTextLight</item>
        <item name="android:textSize">11sp</item>
        <item name="android:textStyle">normal</item>
    </style>
</resources>